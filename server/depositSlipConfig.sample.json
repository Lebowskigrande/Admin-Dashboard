{
  "templatePath": "deposit slip template.pdf",
  "outputPath": "output/deposit-slip.pdf",
  "ocrRegions": {
    "micr": { "xMin": 0.0, "xMax": 1.0, "yMin": 0.84, "yMax": 1.0 },
    "legalAmount": { "xMin": 0.0288, "xMax": 0.8738, "yMin": 0.4435, "yMax": 0.5592 },
    "numericAmount": { "xMin": 0.7513, "xMax": 0.9538, "yMin": 0.3276, "yMax": 0.4700 },
    "checkNumber": { "xMin": 0.8, "xMax": 0.96, "yMin": 0.9, "yMax": 0.95 }
  },
  "ocrRegionOrigin": "top-left",
  "ocrRegionAnchor": "none",
  "ocrEngines": ["trocr"],
  "ocrPreviewOnly": false,
  "ocrModel": "microsoft/trocr-large-handwritten",
  "ocrCropMaxSize": 512,
  "ocrAlign": {
    "enabled": true,
    "boundsPadding": 32,
    "deskewMaxAngle": 3,
    "deskewStep": 0.5,
    "deskewBand": 0.2,
    "deskewScale": 0.4,
    "micrTessLang": "eng"
  },
  "fieldMap": {
    "cash": "cash_amount",
    "subtotal": "total_subtotal",
    "total": ["total_amount", "total_total"],
    "date": "deposit_date",
    "checks": [
      { "number": "check1_number", "amount": "check1_amount" },
      { "number": "check2_number", "amount": "check2_amount" },
      { "number": "check3_number", "amount": "check3_amount" },
      { "number": "check4_number", "amount": "check4_amount" },
      { "number": "check5_number", "amount": "check5_amount" },
      { "number": "check6_number", "amount": "check6_amount" },
      { "number": "check7_number", "amount": "check7_amount" },
      { "number": "check8_number", "amount": "check8_amount" }
    ]
  }
}
